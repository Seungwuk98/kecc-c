// CHECK-NEXT: j b10()

// CHECK-LABEL: block b10:
// CHECK-NEXT: j b1()

// CHECK-LABEL: block b11:
// CHECK-NEXT: j b10()

// CHECK-LABEL: block b12:
// CHECK-NEXT: ret undef:i32
// CHECK-NEXT: }

