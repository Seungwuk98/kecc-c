// RUN: kecc-opt %s -action=parse | FileCheck %s

fun i32 @main () {
init:
  bid: b0
  allocations: 
    %l0:i32:temp

block b0:
  %b0:i0:unit = store 0:i32 %l0:i32*
  %b0:i1:i32 = load %l0:i32*
  %b0:i2:u32 = typecast %b0:i1:i32 to u32
  %b0:i3:u1 = cmp lt %b0:i2:u32 4294967295:u32
  %b0:i4:i32 = typecast %b0:i3:u1 to i32
  ret %b0:i4:i32

block b1:
  ret 0:i32
}


// CHECK: fun i32 @main () {
// CHECK-NEXT: init:
// CHECK-NEXT: bid: b0
// CHECK-NEXT: allocations:
// CHECK-NEXT: %l0:i32:temp

// CHECK-LABEL: block b0:
// CHECK-NEXT: %b0:i0:unit = store 0:i32 %l0:i32*
// CHECK-NEXT: %b0:i1:i32 = load %l0:i32*
// CHECK-NEXT: %b0:i2:u32 = typecast %b0:i1:i32 to u32
// CHECK-NEXT: %b0:i3:u1 = cmp lt %b0:i2:u32 4294967295:u32
// CHECK-NEXT: %b0:i4:i32 = typecast %b0:i3:u1 to i32
// CHECK-NEXT: ret %b0:i4:i32

// CHECK-LABEL: block b1:
// CHECK-NEXT: ret 0:i32
// CHECK-NEXT: }

