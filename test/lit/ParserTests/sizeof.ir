// RUN: kecc-opt %s -action=parse | FileCheck %s

fun i32 @main () {
init:
  bid: b0
  allocations: 


block b0:
  %b0:i0:u64 = typecast 4:i32 to u64
  %b0:i1:u1 = cmp eq 4:u64 %b0:i0:u64
  %b0:i2:i32 = typecast %b0:i1:u1 to i32
  ret %b0:i2:i32

block b1:
  ret 0:i32
}


// CHECK: fun i32 @main () {
// CHECK-NEXT: init:
// CHECK-NEXT: bid: b0
// CHECK-NEXT: allocations:


// CHECK-LABEL: block b0:
// CHECK-NEXT: %b0:i0:u64 = typecast 4:i32 to u64
// CHECK-NEXT: %b0:i1:u1 = cmp eq 4:u64 %b0:i0:u64
// CHECK-NEXT: %b0:i2:i32 = typecast %b0:i1:u1 to i32
// CHECK-NEXT: ret %b0:i2:i32

// CHECK-LABEL: block b1:
// CHECK-NEXT: ret 0:i32
// CHECK-NEXT: }

