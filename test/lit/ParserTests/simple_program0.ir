// RUN: kecc-opt %s -action=parse | FileCheck %s

fun i32 @test_func () {
init:
  bid: b0
  allocations:

block b0:
  %b0:i0:i32 = add 15:i32 25:i32
  j b1(%b0:i0:i32)

block b1:
  %b1:p0:i32
  ret %b1:p0:i32
}

// CHECK: fun i32 @test_func () 
// CHECK-NEXT: init:
// CHECK-NEXT:   bid: b0
// CHECK-NEXT:   allocations:

// CHECK-LABEL: block b0:
// CHECK-NEXT:   %b0:i0:i32 = add 15:i32 25:i32
// CHECK-NEXT:   j b1(%b0:i0:i32)

// CHECK-LABEL: block b1:
// CHECK-NEXT:   %b1:p0:i32
// CHECK-NEXT:   ret %b1:p0:i32
