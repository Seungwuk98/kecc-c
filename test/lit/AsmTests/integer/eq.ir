///// main.c 
// RUN: kecc-test-executor %s | FileCheck %s --check-prefix=RESULT

#include <stdio.h>
#include <stdint.h>

extern int eq_i1 (_Bool, _Bool);
extern int eq_i8 (int8_t, int8_t);
extern int eq_i16(int16_t, int16_t);
extern int eq_i32(int32_t, int32_t);
extern int eq_i64(int64_t, int64_t);

extern int eq_u1 (_Bool, _Bool);
extern int eq_u8 (uint8_t, uint8_t);
extern int eq_u16(uint16_t, uint16_t);
extern int eq_u32(uint32_t, uint32_t);
extern int eq_u64(uint64_t, uint64_t);

int main(void) {
  // ===== i1 =====
  printf("%d\n", eq_i1(0, 0));
  // RESULT: 1
  printf("%d\n", eq_i1(0, 1));
  // RESULT: 0
  printf("%d\n", eq_i1(1, 0));
  // RESULT: 0
  printf("%d\n", eq_i1(1, 1));
  // RESULT: 1

  // ===== u1 =====
  printf("%d\n", eq_u1(0, 0));
  // RESULT: 1
  printf("%d\n", eq_u1(0, 1));
  // RESULT: 0
  printf("%d\n", eq_u1(1, 0));
  // RESULT: 0
  printf("%d\n", eq_u1(1, 1));
  // RESULT: 1

  // ===== i8 =====
  printf("%d\n", eq_i8(-128, -128));
  // RESULT: 1
  printf("%d\n", eq_i8(-128, -127));
  // RESULT: 0
  printf("%d\n", eq_i8(-1, -1));
  // RESULT: 1
  printf("%d\n", eq_i8(0, 0));
  // RESULT: 1
  printf("%d\n", eq_i8(1, -1));
  // RESULT: 0
  printf("%d\n", eq_i8(127, 127));
  // RESULT: 1
  printf("%d\n", eq_i8(100, 27));
  // RESULT: 0
  printf("%d\n", eq_i8(120, 10));
  // RESULT: 0
  printf("%d\n", eq_i8(-50, -50));
  // RESULT: 1
  printf("%d\n", eq_i8(-128, 127));
  // RESULT: 0

  // ===== u8 =====
  printf("%d\n", eq_u8(0u, 0u));
  // RESULT: 1
  printf("%d\n", eq_u8(255u, 255u));
  // RESULT: 1
  printf("%d\n", eq_u8(255u, 0u));
  // RESULT: 0
  printf("%d\n", eq_u8(0u, 255u));
  // RESULT: 0
  printf("%d\n", eq_u8(128u, 128u));
  // RESULT: 1
  printf("%d\n", eq_u8(127u, 128u));
  // RESULT: 0
  printf("%d\n", eq_u8(200u, 55u));
  // RESULT: 0
  printf("%d\n", eq_u8(250u, 10u));
  // RESULT: 0
  printf("%d\n", eq_u8(1u, 2u));
  // RESULT: 0
  printf("%d\n", eq_u8(42u, 42u));
  // RESULT: 1

  // ===== i16 =====
  printf("%d\n", eq_i16(-32768, -32768));
  // RESULT: 1
  printf("%d\n", eq_i16(32767, 32767));
  // RESULT: 1
  printf("%d\n", eq_i16(-32768, 32767));
  // RESULT: 0
  printf("%d\n", eq_i16(30000, 10000));
  // RESULT: 0
  printf("%d\n", eq_i16(10000, 10000));
  // RESULT: 1
  printf("%d\n", eq_i16(-30000, -30000));
  // RESULT: 1
  printf("%d\n", eq_i16(-1, 1));
  // RESULT: 0
  printf("%d\n", eq_i16(0, -1));
  // RESULT: 0
  printf("%d\n", eq_i16(0, 0));
  // RESULT: 1
  printf("%d\n", eq_i16(12345, -12345));
  // RESULT: 0

  // ===== u16 =====
  printf("%d\n", eq_u16(0u, 0u));
  // RESULT: 1
  printf("%d\n", eq_u16(65535u, 65535u));
  // RESULT: 1
  printf("%d\n", eq_u16(65535u, 0u));
  // RESULT: 0
  printf("%d\n", eq_u16(0u, 65535u));
  // RESULT: 0
  printf("%d\n", eq_u16(50000u, 20000u));
  // RESULT: 0
  printf("%d\n", eq_u16(20000u, 20000u));
  // RESULT: 1
  printf("%d\n", eq_u16(32768u, 32768u));
  // RESULT: 1
  printf("%d\n", eq_u16(1u, 65535u));
  // RESULT: 0
  printf("%d\n", eq_u16(2u, 65534u));
  // RESULT: 0
  printf("%d\n", eq_u16(12345u, 54321u));
  // RESULT: 0

  // ===== i32 =====
  printf("%d\n", eq_i32(-2147483648, -2147483648));
  // RESULT: 1
  printf("%d\n", eq_i32(2147483647, 2147483647));
  // RESULT: 1
  printf("%d\n", eq_i32(-2147483648, 2147483647));
  // RESULT: 0
  printf("%d\n", eq_i32(2000000000, 1000000000));
  // RESULT: 0
  printf("%d\n", eq_i32(1000000000, 1000000000));
  // RESULT: 1
  printf("%d\n", eq_i32(-2000000000, -2000000000));
  // RESULT: 1
  printf("%d\n", eq_i32(-1, -1));
  // RESULT: 1
  printf("%d\n", eq_i32(0, -1));
  // RESULT: 0
  printf("%d\n", eq_i32(0, 0));
  // RESULT: 1
  printf("%d\n", eq_i32(123456789, -123456789));
  // RESULT: 0

  // ===== u32 =====
  printf("%d\n", eq_u32(0u, 0u));
  // RESULT: 1
  printf("%d\n", eq_u32(4294967295u, 4294967295u));
  // RESULT: 1
  printf("%d\n", eq_u32(4294967295u, 0u));
  // RESULT: 0
  printf("%d\n", eq_u32(0u, 4294967295u));
  // RESULT: 0
  printf("%d\n", eq_u32(4000000000u, 1000000000u));
  // RESULT: 0
  printf("%d\n", eq_u32(2147483648u, 2147483648u));
  // RESULT: 1
  printf("%d\n", eq_u32(1u, 4294967295u));
  // RESULT: 0
  printf("%d\n", eq_u32(2u, 4294967294u));
  // RESULT: 0
  printf("%d\n", eq_u32(1234567890u, 1234567890u));
  // RESULT: 1
  printf("%d\n", eq_u32(256u, 1u));
  // RESULT: 0

  // ===== i64 =====
  printf("%d\n", eq_i64(-9223372036854775807ll - 1, -9223372036854775807ll - 1));
  // RESULT: 1
  printf("%d\n", eq_i64(9223372036854775807ll, 9223372036854775807ll));
  // RESULT: 1
  printf("%d\n", eq_i64(-9223372036854775807ll - 1, 9223372036854775807ll));
  // RESULT: 0
  printf("%d\n", eq_i64(9000000000000000000ll, 9000000000000000000ll));
  // RESULT: 1
  printf("%d\n", eq_i64(-9000000000000000000ll, -9000000000000000000ll));
  // RESULT: 1
  printf("%d\n", eq_i64(-1ll, 0ll));
  // RESULT: 0
  printf("%d\n", eq_i64(0ll, -1ll));
  // RESULT: 0
  printf("%d\n", eq_i64(0ll, 0ll));
  // RESULT: 1
  printf("%d\n", eq_i64(1234567890123456789ll, 1234567890123456789ll));
  // RESULT: 1
  printf("%d\n", eq_i64(1234567890123456789ll, -1234567890123456789ll));
  // RESULT: 0

  // ===== u64 =====
  printf("%d\n", eq_u64(0ull, 0ull));
  // RESULT: 1
  printf("%d\n", eq_u64(18446744073709551615ull, 18446744073709551615ull));
  // RESULT: 1
  printf("%d\n", eq_u64(18446744073709551615ull, 0ull));
  // RESULT: 0
  printf("%d\n", eq_u64(0ull, 18446744073709551615ull));
  // RESULT: 0
  printf("%d\n", eq_u64(9223372036854775808ull, 9223372036854775808ull));
  // RESULT: 1
  printf("%d\n", eq_u64(18000000000000000000ull, 10000000000000000000ull));
  // RESULT: 0
  printf("%d\n", eq_u64(1ull, 18446744073709551615ull));
  // RESULT: 0
  printf("%d\n", eq_u64(18446744073709551615ull, 1ull));
  // RESULT: 0
  printf("%d\n", eq_u64(42ull, 42ull));
  // RESULT: 1
  printf("%d\n", eq_u64(12345678901234567890ull, 12345678901234567890ull));
  // RESULT: 1

  return 0;
}



