// RUN: kecc-test-executor %s --dump-source=0 | kecc-translate | FileCheck %s

fun i1 @mod_i1 (i1, i1) {
init:
  bid: b0
  allocations:

block b0:
  %b0:p0:i1
  %b0:p1:i1
  %b0:i0:i1 = mod %b0:p0:i1 %b0:p1:i1
  ret %b0:i0:i1
}

// CHECK:  .globl	mod_i1
// CHECK-NEXT:   .align	1
// CHECK-NEXT:   .type	mod_i1, @function
// CHECK-NEXT: mod_i1:

// CHECK-LABEL: .mod_i1_L0:
// CHECK-NEXT:   rem	a0,a0,a1
// CHECK-NEXT:   ret


