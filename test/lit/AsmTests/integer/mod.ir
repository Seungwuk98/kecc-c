///// main.c

// RUN: kecc-test-executor %s | FileCheck %s --check-prefix=RESULT

#include <stdio.h>
#include <stdint.h>

extern _Bool   mod_i1(_Bool, _Bool);
extern int8_t  mod_i8(int8_t, int8_t);
extern int16_t mod_i16(int16_t, int16_t);
extern int32_t mod_i32(int32_t, int32_t);
extern int64_t mod_i64(int64_t, int64_t);

extern _Bool    mod_u1(_Bool, _Bool);
extern uint8_t  mod_u8(uint8_t, uint8_t);
extern uint16_t mod_u16(uint16_t, uint16_t);
extern uint32_t mod_u32(uint32_t, uint32_t);
extern uint64_t mod_u64(uint64_t, uint64_t);

int main(void) {
  // ===== i1 (divisor == 1) =====
  printf("%d\n", (int) mod_i1(0, 1));
  // RESULT: 0
  printf("%d\n", (int) mod_i1(1, 1));
  // RESULT: 0

  // ===== u1 =====
  printf("%d\n", (int) mod_u1(0, 1));
  // RESULT: 0
  printf("%d\n", (int) mod_u1(1, 1));
  // RESULT: 0

  // ===== i8 =====
  printf("%d\n", (int) mod_i8(0, 1));
  // RESULT: 0
  printf("%d\n", (int) mod_i8(127, 1));
  // RESULT: 0
  printf("%d\n", (int) mod_i8(-128, 1));
  // RESULT: 0
  printf("%d\n", (int) mod_i8(-128, 2));
  // RESULT: 0
  printf("%d\n", (int) mod_i8(127, 2));
  // RESULT: 1
  printf("%d\n", (int) mod_i8(-127, 2));
  // RESULT: -1
  printf("%d\n", (int) mod_i8(100, 3));
  // RESULT: 1
  printf("%d\n", (int) mod_i8(-100, 3));
  // RESULT: -1
  printf("%d\n", (int) mod_i8(-100, -3));
  // RESULT: -1
  printf("%d\n", (int) mod_i8(1, 127));
  // RESULT: 1
  printf("%d\n", (int) mod_i8(-1, 127));
  // RESULT: -1
  printf("%d\n", (int) mod_i8(-1, -128));
  // RESULT: -1
  printf("%d\n", (int) mod_i8(120, 10));
  // RESULT: 0
  printf("%d\n", (int) mod_i8(120, -10));
  // RESULT: 0
  printf("%d\n", (int) mod_i8(-128, 7));
  // RESULT: -2
  printf("%d\n", (int) mod_i8(127, 127));
  // RESULT: 0

  // ===== u8 =====
  printf("%u\n", (unsigned) mod_u8(255u, 1u));
  // RESULT: 0
  printf("%u\n", (unsigned) mod_u8(255u, 2u));
  // RESULT: 1
  printf("%u\n", (unsigned) mod_u8(200u, 3u));
  // RESULT: 2
  printf("%u\n", (unsigned) mod_u8(1u, 255u));
  // RESULT: 1
  printf("%u\n", (unsigned) mod_u8(0u, 255u));
  // RESULT: 0
  printf("%u\n", (unsigned) mod_u8(255u, 255u));
  // RESULT: 0
  printf("%u\n", (unsigned) mod_u8(128u, 3u));
  // RESULT: 2
  printf("%u\n", (unsigned) mod_u8(250u, 10u));
  // RESULT: 0
  printf("%u\n", (unsigned) mod_u8(250u, 11u));
  // RESULT: 8
  printf("%u\n", (unsigned) mod_u8(128u, 5u));
  // RESULT: 3
  printf("%u\n", (unsigned) mod_u8(2u, 255u));
  // RESULT: 2
  printf("%u\n", (unsigned) mod_u8(254u, 255u));
  // RESULT: 254

  // ===== i16 =====
  printf("%d\n", (int) mod_i16(32767, 1));
  // RESULT: 0
  printf("%d\n", (int) mod_i16(-32768, 1));
  // RESULT: 0
  printf("%d\n", (int) mod_i16(32767, 2));
  // RESULT: 1
  printf("%d\n", (int) mod_i16(-32768, 2));
  // RESULT: 0
  printf("%d\n", (int) mod_i16(-32767, 2));
  // RESULT: -1
  printf("%d\n", (int) mod_i16(30000, 10000));
  // RESULT: 0
  printf("%d\n", (int) mod_i16(-30000, 10000));
  // RESULT: 0
  printf("%d\n", (int) mod_i16(30000, 7));
  // RESULT: 5
  printf("%d\n", (int) mod_i16(-30000, 7));
  // RESULT: -5
  printf("%d\n", (int) mod_i16(-32768, 7));
  // RESULT: -1
  printf("%d\n", (int) mod_i16(32767, -32768));
  // RESULT: 32767
  printf("%d\n", (int) mod_i16(-32768, -32768));
  // RESULT: 0
  printf("%d\n", (int) mod_i16(12345, -12345));
  // RESULT: 0

  // ===== u16 =====
  printf("%u\n", (unsigned) mod_u16(65535u, 1u));
  // RESULT: 0
  printf("%u\n", (unsigned) mod_u16(65535u, 2u));
  // RESULT: 1
  printf("%u\n", (unsigned) mod_u16(50000u, 20000u));
  // RESULT: 10000
  printf("%u\n", (unsigned) mod_u16(50000u, 65535u));
  // RESULT: 50000
  printf("%u\n", (unsigned) mod_u16(1u, 65535u));
  // RESULT: 1
  printf("%u\n", (unsigned) mod_u16(65535u, 65535u));
  // RESULT: 0
  printf("%u\n", (unsigned) mod_u16(32768u, 3u));
  // RESULT: 2
  printf("%u\n", (unsigned) mod_u16(65535u, 3u));
  // RESULT: 0
  printf("%u\n", (unsigned) mod_u16(0u, 65535u));
  // RESULT: 0
  printf("%u\n", (unsigned) mod_u16(2u, 65534u));
  // RESULT: 2
  printf("%u\n", (unsigned) mod_u16(12345u, 54321u));
  // RESULT: 12345

  // ===== i32 =====
  printf("%d\n", (int) mod_i32(2147483647, 1));
  // RESULT: 0
  printf("%d\n", (int) mod_i32(-2147483648, 1));
  // RESULT: 0
  printf("%d\n", (int) mod_i32(-2147483648, 2));
  // RESULT: 0
  printf("%d\n", (int) mod_i32(2147483647, 2));
  // RESULT: 1
  printf("%d\n", (int) mod_i32(-2147483647, 2));
  // RESULT: -1
  printf("%d\n", (int) mod_i32(2000000000, 3));
  // RESULT: 2
  printf("%d\n", (int) mod_i32(-2000000000, 3));
  // RESULT: -2
  printf("%d\n", (int) mod_i32(1, 2147483647));
  // RESULT: 1
  printf("%d\n", (int) mod_i32(0, -2147483648));
  // RESULT: 0
  printf("%d\n", (int) mod_i32(0x40000000, 2));
  // RESULT: 0
  printf("%d\n", (int) mod_i32(2147483647, 1073741824));
  // RESULT: 1073741823
  printf("%d\n", (int) mod_i32(-2147483648, 1073741824));
  // RESULT: 0
  printf("%d\n", (int) mod_i32(1234567890, 1000));
  // RESULT: 890
  printf("%d\n", (int) mod_i32(-15, -2));
  // RESULT: 1
  printf("%d\n", (int) mod_i32(15, -2));
  // RESULT: 1
  
  // ===== u32 =====
  printf("%u\n", (unsigned) mod_u32(4294967295u, 1u));
  // RESULT: 0
  printf("%u\n", (unsigned) mod_u32(4294967295u, 2u));
  // RESULT: 1
  printf("%u\n", (unsigned) mod_u32(4000000000u, 3u));
  // RESULT: 1
  printf("%u\n", (unsigned) mod_u32(1u, 4294967295u));
  // RESULT: 1
  printf("%u\n", (unsigned) mod_u32(0u, 4294967295u));
  // RESULT: 0
  printf("%u\n", (unsigned) mod_u32(4294967295u, 4294967295u));
  // RESULT: 0
  printf("%u\n", (unsigned) mod_u32(2147483648u, 3u));
  // RESULT: 2
  printf("%u\n", (unsigned) mod_u32(2147483648u, 2147483648u));
  // RESULT: 0
  printf("%u\n", (unsigned) mod_u32(3000000000u, 1000000000u));
  // RESULT: 0
  printf("%u\n", (unsigned) mod_u32(1234567890u, 1000u));
  // RESULT: 890
  printf("%u\n", (unsigned) mod_u32(15, -2));
  // RESULT: 15

  // ===== i64 =====
  printf("%lld\n", (long long) mod_i64(9223372036854775807ll, 1ll));
  // RESULT: 0
  printf("%lld\n", (long long) mod_i64(-9223372036854775808ll, 1ll));
  // RESULT: 0
  printf("%lld\n", (long long) mod_i64(-9223372036854775808ll, 2ll));
  // RESULT: 0
  printf("%lld\n", (long long) mod_i64(9223372036854775807ll, 2ll));
  // RESULT: 1
  printf("%lld\n", (long long) mod_i64(-9223372036854775807ll, 2ll));
  // RESULT: -1
  printf("%lld\n", (long long) mod_i64(9000000000000000000ll, 3ll));
  // RESULT: 0
  printf("%lld\n", (long long) mod_i64(-9000000000000000000ll, 3ll));
  // RESULT: 0
  printf("%lld\n", (long long) mod_i64(1ll, 9223372036854775807ll));
  // RESULT: 1
  printf("%lld\n", (long long) mod_i64(0ll, -9223372036854775807ll));
  // RESULT: 0
  printf("%lld\n", (long long) mod_i64(0x4000000000000000ll, 2ll));
  // RESULT: 0
  printf("%lld\n", (long long) mod_i64(-9223372036854775808ll, 3ll));
  // RESULT: -2
  printf("%lld\n", (long long) mod_i64(9223372036854775807ll, 3ll));
  // RESULT: 1
  printf("%lld\n", (long long) mod_i64(9000000000000000001ll, 10ll));
  // RESULT: 1

  // ===== u64 =====
  printf("%llu\n", (unsigned long long) mod_u64(18446744073709551615ull, 1ull));
  // RESULT: 0
  printf("%llu\n", (unsigned long long) mod_u64(18446744073709551615ull, 2ull));
  // RESULT: 1
  printf("%llu\n", (unsigned long long) mod_u64(18000000000000000000ull, 3ull));
  // RESULT: 0
  printf("%llu\n", (unsigned long long) mod_u64(1ull, 18446744073709551615ull));
  // RESULT: 1
  printf("%llu\n", (unsigned long long) mod_u64(0ull, 18446744073709551615ull));
  // RESULT: 0
  printf("%llu\n", (unsigned long long) mod_u64(18446744073709551615ull, 18446744073709551615ull));
  // RESULT: 0
  printf("%llu\n", (unsigned long long) mod_u64(9223372036854775808ull, 3ull));
  // RESULT: 2
  printf("%llu\n", (unsigned long long) mod_u64(9223372036854775808ull, 9223372036854775808ull));
  // RESULT: 0
  printf("%llu\n", (unsigned long long) mod_u64(12345678901234567890ull, 10ull));
  // RESULT: 0
  printf("%llu\n", (unsigned long long) mod_u64(2ull, 18446744073709551615ull));
  // RESULT: 2

  return 0;
}

