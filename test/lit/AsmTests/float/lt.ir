// RUN: kecc-test-executor %s -dump-source=0 | kecc-translate | FileCheck %s


fun i1 @lt_f32 (f32, f32) {
init:
  bid: b0
  allocations:

block b0:
  %b0:p0:f32
  %b0:p1:f32
  %b0:i0:i1 = cmp lt %b0:p0:f32 %b0:p1:f32
  ret %b0:i0:i1
}

// CHECK:   .globl	lt_f32
// CHECK-NEXT:   .align	1
// CHECK-NEXT:   .type	lt_f32, @function
// CHECK-NEXT: lt_f32:

// CHECK-LABEL: .lt_f32_L0:
// CHECK-NEXT:   flt.s	a0,fa0,fa1
// CHECK-NEXT:   ret


fun i1 @lt_f64 (f64, f64) {
init:
  bid: b0
  allocations:

block b0:
  %b0:p0:f64
  %b0:p1:f64
  %b0:i0:i1 = cmp lt %b0:p0:f64 %b0:p1:f64
  ret %b0:i0:i1
}


// CHECK:   .globl	lt_f64
// CHECK-NEXT:   .align	1
// CHECK-NEXT:   .type	lt_f64, @function
// CHECK-NEXT: lt_f64:

// CHECK-LABEL: .lt_f64_L0:
// CHECK-NEXT:   flt.d	a0,fa0,fa1
// CHECK-NEXT:   ret


////////////// main.c
// RUN: kecc-test-executor %s | FileCheck %s --check-prefix=RESULT
#include <stdio.h>
#include <stdint.h>
#include <math.h>

extern int lt_f32(float, float);
extern int lt_f64(double, double);

int main(void) {
  // ===== f32 (float) =====
  printf("%d\n", lt_f32(0.0f, 0.0f));
  // RESULT: 0
  printf("%d\n", lt_f32(-0.0f, 0.0f));
  // RESULT: 0
  printf("%d\n", lt_f32(0.0f, -0.0f));
  // RESULT: 0
  printf("%d\n", lt_f32(-0.0f, 0x1p-149f));
  // RESULT: 1
  printf("%d\n", lt_f32(0x1p-149f, 0.0f));
  // RESULT: 0
  printf("%d\n", lt_f32(-0x1p-149f, 0.0f));
  // RESULT: 1

  printf("%d\n", lt_f32(1.0f, 1.0f));
  // RESULT: 0
  printf("%d\n", lt_f32(1.0f, 0x1.000002p+0f));      // 1 < 1 + ulp
  // RESULT: 1
  printf("%d\n", lt_f32(0x1.000002p+0f, 1.0f));
  // RESULT: 0
  printf("%d\n", lt_f32(-1.0f, 1.0f));
  // RESULT: 1
  printf("%d\n", lt_f32(1.0f, -1.0f));
  // RESULT: 0

  printf("%d\n", lt_f32(0x1.fffffep+127f, INFINITY));
  // RESULT: 1
  printf("%d\n", lt_f32(INFINITY, INFINITY));
  // RESULT: 0
  printf("%d\n", lt_f32(-INFINITY, -INFINITY));
  // RESULT: 0
  printf("%d\n", lt_f32(-INFINITY, 0.0f));
  // RESULT: 1
  printf("%d\n", lt_f32(0.0f, -INFINITY));
  // RESULT: 0
  printf("%d\n", lt_f32(-INFINITY, INFINITY));
  // RESULT: 1

  printf("%d\n", lt_f32(NAN, 0.0f));
  // RESULT: 0
  printf("%d\n", lt_f32(0.0f, NAN));
  // RESULT: 0
  printf("%d\n", lt_f32(-0x1.fffffep+127f, 0x1.fffffep+127f));
  // RESULT: 1

  // ===== f64 (double) =====
  printf("%d\n", lt_f64(0.0, 0.0));
  // RESULT: 0
  printf("%d\n", lt_f64(-0.0, 0.0));
  // RESULT: 0
  printf("%d\n", lt_f64(0.0, -0.0));
  // RESULT: 0
  printf("%d\n", lt_f64(-0.0, 0x1p-1074));
  // RESULT: 1
  printf("%d\n", lt_f64(0x1p-1074, 0.0));
  // RESULT: 0
  printf("%d\n", lt_f64(-0x1p-1074, 0.0));
  // RESULT: 1

  printf("%d\n", lt_f64(1.0, 1.0));
  // RESULT: 0
  printf("%d\n", lt_f64(1.0, 0x1.0000000000001p+0));   // 1 < 1 + ulp
  // RESULT: 1
  printf("%d\n", lt_f64(0x1.0000000000001p+0, 1.0));
  // RESULT: 0
  printf("%d\n", lt_f64(-1.0, 1.0));
  // RESULT: 1
  printf("%d\n", lt_f64(1.0, -1.0));
  // RESULT: 0

  printf("%d\n", lt_f64(0x1.fffffffffffffp+1023, INFINITY));
  // RESULT: 1
  printf("%d\n", lt_f64(INFINITY, INFINITY));
  // RESULT: 0
  printf("%d\n", lt_f64(-INFINITY, -INFINITY));
  // RESULT: 0
  printf("%d\n", lt_f64(-INFINITY, 0.0));
  // RESULT: 1
  printf("%d\n", lt_f64(0.0, -INFINITY));
  // RESULT: 0
  printf("%d\n", lt_f64(-INFINITY, INFINITY));
  // RESULT: 1

  printf("%d\n", lt_f64(NAN, 0.0));
  // RESULT: 0
  printf("%d\n", lt_f64(0.0, NAN));
  // RESULT: 0
  printf("%d\n", lt_f64(-0x1.fffffffffffffp+1023, 0x1.fffffffffffffp+1023));
  // RESULT: 1

  return 0;
}

