// RUN: kecc-test-executor %s --test-return-value=7 
// RUN: keci %s --test-return-value=7
// RUN: kecc-opt %s --mem2reg | FileCheck %s


fun i32 @main () {
init:
  bid: b0
  allocations: 
    %l0:i32:a
    %l1:i32:b

block b0:
  %b0:i0:unit = store 3:i32 %l0:i32*
  %b0:i1:i32 = typecast 4:u64 to i32
  %b0:i2:unit = store %b0:i1:i32 %l1:i32*
  %b0:i3:i32 = load %l0:i32*
  %b0:i4:i32 = load %l1:i32*
  %b0:i5:i32 = add %b0:i3:i32 %b0:i4:i32
  ret %b0:i5:i32
}
// CHECK: fun i32 @main () {
// CHECK-NEXT: init:
// CHECK-NEXT:   bid: b0
// CHECK-NEXT:   allocations:
// CHECK-NEXT:     %l0:i32:a
// CHECK-NEXT:     %l1:i32:b

// CHECK-LABEL: block b0:
// CHECK-NEXT:   %b0:i0:unit = nop
// CHECK-NEXT:   %b0:i1:i32 = typecast 4:u64 to i32
// CHECK-NEXT:   %b0:i2:unit = nop
// CHECK-NEXT:   %b0:i3:unit = nop
// CHECK-NEXT:   %b0:i4:unit = nop
// CHECK-NEXT:   %b0:i5:i32 = add 3:i32 %b0:i1:i32
// CHECK-NEXT:   ret %b0:i5:i32
// CHECK-NEXT: }
