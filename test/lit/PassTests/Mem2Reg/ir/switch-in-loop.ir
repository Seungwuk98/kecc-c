// RUN: kecc-test-executor %s --test-return-value=9
// RUN: keci %s --test-return-value=9
// RUN: kecc-opt %s --mem2reg | FileCheck %s


fun i32 @main () {
init:
  bid: b0
  allocations: 
    %l0:i32:i
    %l1:i32:c

block b0:
  %b0:i0:unit = store 0:i32 %l0:i32*
  %b0:i1:unit = store 0:i32 %l1:i32*
  j b1()

block b1:
  %b1:i0:i32 = load %l0:i32*
  %b1:i1:u1 = cmp lt %b1:i0:i32 10:i32
  br %b1:i1:u1, b2(), b3()

block b2:
  %b2:i0:i32 = load %l0:i32*
  %b2:i1:i32 = add %b2:i0:i32 1:i32
  %b2:i2:unit = store %b2:i1:i32 %l0:i32*
  %b2:i3:i32 = load %l0:i32*
  switch %b2:i3:i32 default b7() [
    1:i32 b1()
  ]

block b3:
  %b3:i0:i32 = load %l1:i32*
  ret %b3:i0:i32

block b7:
  %b7:i0:i32 = load %l1:i32*
  %b7:i1:i32 = add %b7:i0:i32 1:i32
  %b7:i2:unit = store %b7:i1:i32 %l1:i32*
  j b1()
}
// CHECK: fun i32 @main () {
// CHECK-NEXT: init:
// CHECK-NEXT:   bid: b0
// CHECK-NEXT:   allocations:
// CHECK-NEXT:     %l0:i32:i
// CHECK-NEXT:     %l1:i32:c

// CHECK-LABEL: block b0:
// CHECK-NEXT:   %b0:i0:unit = nop
// CHECK-NEXT:   %b0:i1:unit = nop
// CHECK-NEXT:   j b1(0:i32, 0:i32)

// CHECK-LABEL: block b1:
// CHECK-NEXT:   %b1:p0:i32:i
// CHECK-NEXT:   %b1:p1:i32:c
// CHECK-NEXT:   %b1:i0:unit = nop
// CHECK-NEXT:   %b1:i1:u1 = cmp lt %b1:p0:i32 10:i32
// CHECK-NEXT:   br %b1:i1:u1, b2(), b3()

// CHECK-LABEL: block b2:
// CHECK-NEXT:   %b2:i0:unit = nop
// CHECK-NEXT:   %b2:i1:i32 = add %b1:p0:i32 1:i32
// CHECK-NEXT:   %b2:i2:unit = nop
// CHECK-NEXT:   %b2:i3:unit = nop
// CHECK-NEXT:   switch %b2:i1:i32 default b7() [
// CHECK-NEXT:     1:i32 b1(%b2:i1:i32, %b1:p1:i32)
// CHECK-NEXT:   ]

// CHECK-LABEL: block b3:
// CHECK-NEXT:   %b3:i0:unit = nop
// CHECK-NEXT:   ret %b1:p1:i32

// CHECK-LABEL: block b7:
// CHECK-NEXT:   %b7:i0:unit = nop
// CHECK-NEXT:   %b7:i1:i32 = add %b1:p1:i32 1:i32
// CHECK-NEXT:   %b7:i2:unit = nop
// CHECK-NEXT:   j b1(%b2:i1:i32, %b7:i1:i32)
// CHECK-NEXT: }
