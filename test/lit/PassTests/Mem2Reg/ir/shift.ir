// RUN: kecc-test-executor %s --test-return-value=1 
// RUN: kecc-opt %s --mem2reg | FileCheck %s


fun i32 @main () {
init:
  bid: b0
  allocations: 
    %l0:i8:a
    %l1:i8:b
    %l2:u8:c
    %l3:u1:t0

block b0:
  %b0:i0:i8 = typecast 127:i32 to i8
  %b0:i1:unit = store %b0:i0:i8 %l0:i8*
  %b0:i2:i8 = load %l0:i8*
  %b0:i3:i32 = typecast %b0:i2:i8 to i32
  %b0:i4:i32 = shl %b0:i3:i32 1:i32
  %b0:i5:i8 = typecast %b0:i4:i32 to i8
  %b0:i6:unit = store %b0:i5:i8 %l1:i8*
  %b0:i7:i8 = load %l1:i8*
  %b0:i8:u8 = typecast %b0:i7:i8 to u8
  %b0:i9:i32 = typecast %b0:i8:u8 to i32
  %b0:i10:i32 = shr %b0:i9:i32 1:i32
  %b0:i11:u8 = typecast %b0:i10:i32 to u8
  %b0:i12:unit = store %b0:i11:u8 %l2:u8*
  %b0:i13:i8 = load %l1:i8*
  %b0:i14:i32 = minus 2:i32
  %b0:i15:i32 = typecast %b0:i13:i8 to i32
  %b0:i16:u1 = cmp eq %b0:i15:i32 %b0:i14:i32
  br %b0:i16:u1, b1(), b2()

block b1:
  %b1:i0:u8 = load %l2:u8*
  %b1:i1:i32 = typecast %b1:i0:u8 to i32
  %b1:i2:u1 = cmp eq %b1:i1:i32 127:i32
  %b1:i3:unit = store %b1:i2:u1 %l3:u1*
  j b3()

block b2:
  %b2:i0:unit = store 0:u1 %l3:u1*
  j b3()

block b3:
  %b3:i0:u1 = load %l3:u1*
  %b3:i1:i32 = typecast %b3:i0:u1 to i32
  ret %b3:i1:i32
}
// CHECK: fun i32 @main () {
// CHECK-NEXT: init:
// CHECK-NEXT:   bid: b0
// CHECK-NEXT:   allocations:
// CHECK-NEXT:     %l0:i8:a
// CHECK-NEXT:     %l1:i8:b
// CHECK-NEXT:     %l2:u8:c
// CHECK-NEXT:     %l3:u1:t0

// CHECK-LABEL: block b0:
// CHECK-NEXT:   %b0:i0:i8 = typecast 127:i32 to i8
// CHECK-NEXT:   %b0:i1:unit = nop
// CHECK-NEXT:   %b0:i2:unit = nop
// CHECK-NEXT:   %b0:i3:i32 = typecast %b0:i0:i8 to i32
// CHECK-NEXT:   %b0:i4:i32 = shl %b0:i3:i32 1:i32
// CHECK-NEXT:   %b0:i5:i8 = typecast %b0:i4:i32 to i8
// CHECK-NEXT:   %b0:i6:unit = nop
// CHECK-NEXT:   %b0:i7:unit = nop
// CHECK-NEXT:   %b0:i8:u8 = typecast %b0:i5:i8 to u8
// CHECK-NEXT:   %b0:i9:i32 = typecast %b0:i8:u8 to i32
// CHECK-NEXT:   %b0:i10:i32 = shr %b0:i9:i32 1:i32
// CHECK-NEXT:   %b0:i11:u8 = typecast %b0:i10:i32 to u8
// CHECK-NEXT:   %b0:i12:unit = nop
// CHECK-NEXT:   %b0:i13:unit = nop
// CHECK-NEXT:   %b0:i14:i32 = minus 2:i32
// CHECK-NEXT:   %b0:i15:i32 = typecast %b0:i5:i8 to i32
// CHECK-NEXT:   %b0:i16:u1 = cmp eq %b0:i15:i32 %b0:i14:i32
// CHECK-NEXT:   br %b0:i16:u1, b1(), b2()

// CHECK-LABEL: block b1:
// CHECK-NEXT:   %b1:i0:unit = nop
// CHECK-NEXT:   %b1:i1:i32 = typecast %b0:i11:u8 to i32
// CHECK-NEXT:   %b1:i2:u1 = cmp eq %b1:i1:i32 127:i32
// CHECK-NEXT:   %b1:i3:unit = nop
// CHECK-NEXT:   j b3(%b1:i2:u1)

// CHECK-LABEL: block b2:
// CHECK-NEXT:   %b2:i0:unit = nop
// CHECK-NEXT:   j b3(0:u1)

// CHECK-LABEL: block b3:
// CHECK-NEXT:   %b3:p0:u1:t0
// CHECK-NEXT:   %b3:i0:unit = nop
// CHECK-NEXT:   %b3:i1:i32 = typecast %b3:p0:u1 to i32
// CHECK-NEXT:   ret %b3:i1:i32
// CHECK-NEXT: }
