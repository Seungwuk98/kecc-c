// RUN: kecc-test-executor %s --test-return-value=1
// RUN: keci %s --test-return-value=1
// RUN: kecc-opt %s --mem2reg | FileCheck %s


fun i32 @bar (i32, i32, i32) {
init:
  bid: b0
  allocations: 
    %l0:i32:x
    %l1:i32:y
    %l2:i32:z
    %l3:i32:arith_mean
    %l4:i32:ugly_mean

block b0:
  %b0:p0:i32:x
  %b0:p1:i32:y
  %b0:p2:i32:z
  %b0:i0:unit = store %b0:p0:i32 %l0:i32*
  %b0:i1:unit = store %b0:p1:i32 %l1:i32*
  %b0:i2:unit = store %b0:p2:i32 %l2:i32*
  %b0:i3:i32 = load %l0:i32*
  %b0:i4:i32 = load %l1:i32*
  %b0:i5:i32 = add %b0:i3:i32 %b0:i4:i32
  %b0:i6:i32 = load %l2:i32*
  %b0:i7:i32 = add %b0:i5:i32 %b0:i6:i32
  %b0:i8:i32 = div %b0:i7:i32 3:i32
  %b0:i9:unit = store %b0:i8:i32 %l3:i32*
  %b0:i10:i32 = load %l0:i32*
  %b0:i11:i32 = load %l1:i32*
  %b0:i12:i32 = add %b0:i10:i32 %b0:i11:i32
  %b0:i13:i32 = div %b0:i12:i32 2:i32
  %b0:i14:i32 = mul %b0:i13:i32 2:i32
  %b0:i15:i32 = load %l2:i32*
  %b0:i16:i32 = add %b0:i14:i32 %b0:i15:i32
  %b0:i17:i32 = div %b0:i16:i32 3:i32
  %b0:i18:unit = store %b0:i17:i32 %l4:i32*
  %b0:i19:i32 = load %l0:i32*
  %b0:i20:i32 = load %l1:i32*
  %b0:i21:u1 = cmp eq %b0:i19:i32 %b0:i20:i32
  br %b0:i21:u1, b1(), b2()

block b1:
  %b1:i0:i32 = load %l1:i32*
  ret %b1:i0:i32

block b2:
  %b2:i0:i32 = load %l2:i32*
  ret %b2:i0:i32
}
// CHECK: fun i32 @bar (i32, i32, i32) {
// CHECK-NEXT: init:
// CHECK-NEXT:   bid: b0
// CHECK-NEXT:   allocations:
// CHECK-NEXT:     %l0:i32:x
// CHECK-NEXT:     %l1:i32:y
// CHECK-NEXT:     %l2:i32:z
// CHECK-NEXT:     %l3:i32:arith_mean
// CHECK-NEXT:     %l4:i32:ugly_mean

// CHECK-LABEL: block b0:
// CHECK-NEXT:   %b0:p0:i32:x
// CHECK-NEXT:   %b0:p1:i32:y
// CHECK-NEXT:   %b0:p2:i32:z
// CHECK-NEXT:   %b0:i0:unit = nop
// CHECK-NEXT:   %b0:i1:unit = nop
// CHECK-NEXT:   %b0:i2:unit = nop
// CHECK-NEXT:   %b0:i3:unit = nop
// CHECK-NEXT:   %b0:i4:unit = nop
// CHECK-NEXT:   %b0:i5:i32 = add %b0:p0:i32 %b0:p1:i32
// CHECK-NEXT:   %b0:i6:unit = nop
// CHECK-NEXT:   %b0:i7:i32 = add %b0:i5:i32 %b0:p2:i32
// CHECK-NEXT:   %b0:i8:i32 = div %b0:i7:i32 3:i32
// CHECK-NEXT:   %b0:i9:unit = nop
// CHECK-NEXT:   %b0:i10:unit = nop
// CHECK-NEXT:   %b0:i11:unit = nop
// CHECK-NEXT:   %b0:i12:i32 = add %b0:p0:i32 %b0:p1:i32
// CHECK-NEXT:   %b0:i13:i32 = div %b0:i12:i32 2:i32
// CHECK-NEXT:   %b0:i14:i32 = mul %b0:i13:i32 2:i32
// CHECK-NEXT:   %b0:i15:unit = nop
// CHECK-NEXT:   %b0:i16:i32 = add %b0:i14:i32 %b0:p2:i32
// CHECK-NEXT:   %b0:i17:i32 = div %b0:i16:i32 3:i32
// CHECK-NEXT:   %b0:i18:unit = nop
// CHECK-NEXT:   %b0:i19:unit = nop
// CHECK-NEXT:   %b0:i20:unit = nop
// CHECK-NEXT:   %b0:i21:u1 = cmp eq %b0:p0:i32 %b0:p1:i32
// CHECK-NEXT:   br %b0:i21:u1, b1(), b2()

// CHECK-LABEL: block b1:
// CHECK-NEXT:   %b1:i0:unit = nop
// CHECK-NEXT:   ret %b0:p1:i32

// CHECK-LABEL: block b2:
// CHECK-NEXT:   %b2:i0:unit = nop
// CHECK-NEXT:   ret %b0:p2:i32
// CHECK-NEXT: }

fun i32 @main () {
init:
  bid: b0
  allocations: 


block b0:
  ret 1:i32
}
