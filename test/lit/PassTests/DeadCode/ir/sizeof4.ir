// RUN: kecc-test-executor %s --test-return-value=7 
// RUN: kecc-opt %s --dead-code-elimination | FileCheck %s


fun i32 @main () {
init:
  bid: b0
  allocations: 
    %l0:i32:a
    %l1:i32:b

block b0:
  %b0:i0:unit = nop
  %b0:i1:i32 = typecast 4:u64 to i32
  %b0:i2:unit = nop
  %b0:i3:unit = nop
  %b0:i4:unit = nop
  %b0:i5:i32 = add 3:i32 %b0:i1:i32
  ret %b0:i5:i32
}
// CHECK: fun i32 @main () {
// CHECK-NEXT: init:
// CHECK-NEXT:   bid: b0
// CHECK-NEXT:   allocations:


// CHECK-LABEL: block b0:
// CHECK-NEXT:   %b0:i0:i32 = typecast 4:u64 to i32
// CHECK-NEXT:   %b0:i1:i32 = add 3:i32 %b0:i0:i32
// CHECK-NEXT:   ret %b0:i1:i32
// CHECK-NEXT: }
