// RUN: kecc-opt %s --dead-code-elimination | FileCheck %s

var i32 @nonce = 1
// CHECK: var i32 @nonce = 1

fun i32 @main () {
init:
  bid: b0
  allocations: 
    %l0:i32:x

block b0:
  %b0:i0:i32 = load @nonce:i32*
  %b0:i1:unit = nop
  %b0:i2:unit = nop
  ret %b0:i0:i32
}
// CHECK-LABEL: fun i32 @main () {
// CHECK-NEXT: init:
// CHECK-NEXT:   bid: b0
// CHECK-NEXT:   allocations:


// CHECK-LABEL: block b0:
// CHECK-NEXT:   %b0:i0:i32 = load @nonce:i32*
// CHECK-NEXT:   ret %b0:i0:i32
// CHECK-NEXT: }
