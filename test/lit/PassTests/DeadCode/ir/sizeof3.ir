// RUN: kecc-opt %s --dead-code-elimination | FileCheck %s


fun i32 @main () {
init:
  bid: b0
  allocations: 
    %l0:i32:x
    %l1:i32:y

block b0:
  %b0:i0:unit = nop
  %b0:i1:i32 = typecast 4:u64 to i32
  %b0:i2:unit = nop
  %b0:i3:unit = nop
  ret 3:i32
}
// CHECK: fun i32 @main () {
// CHECK-NEXT: init:
// CHECK-NEXT:   bid: b0
// CHECK-NEXT:   allocations:


// CHECK-LABEL: block b0:
// CHECK-NEXT:   ret 3:i32
// CHECK-NEXT: }
