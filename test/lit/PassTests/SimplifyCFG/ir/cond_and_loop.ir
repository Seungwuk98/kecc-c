// RUN: kecc-opt %s --canonicalize-constant --simplify-cfg | FileCheck %s

var i32 @nonce = 1
// CHECK: var i32 @nonce = 1

fun i32 @main () {
init:
  bid: b0
  allocations: 
    %l0:i32:i
    %l1:i32:p
    %l2:i32:q
    %l3:i32:r
    %l4:i32:t0
    %l5:i32:t1
    %l6:i32:loop_num
    %l7:i32:t2

block b0:
  %b0:i0:unit = store 2:i32 %l1:i32*
  %b0:i1:unit = store 5:i32 %l2:i32*
  %b0:i2:u1 = cmp ne 0:i32 0:i32
  br %b0:i2:u1, b1(), b2()

block b1:
  %b1:i0:i32 = load %l1:i32*
  %b1:i1:i32 = load %l2:i32*
  %b1:i2:u1 = cmp gt %b1:i0:i32 %b1:i1:i32
  br %b1:i2:u1, b4(), b5()

block b2:
  %b2:i0:i32 = load %l1:i32*
  %b2:i1:i32 = load %l2:i32*
  %b2:i2:i32 = add %b2:i0:i32 %b2:i1:i32
  %b2:i3:unit = store %b2:i2:i32 %l5:i32*
  j b3()

block b3:
  %b3:i0:i32 = load %l5:i32*
  %b3:i1:unit = store %b3:i0:i32 %l3:i32*
  %b3:i2:i32 = load @nonce:i32*
  %b3:i3:i32 = mod %b3:i2:i32 100:i32
  %b3:i4:unit = store %b3:i3:i32 %l6:i32*
  j b7()

block b4:
  %b4:i0:i32 = load %l1:i32*
  %b4:i1:i32 = sub %b4:i0:i32 2:i32
  %b4:i2:unit = store %b4:i1:i32 %l1:i32*
  %b4:i3:unit = store %b4:i1:i32 %l4:i32*
  j b6()

block b5:
  %b5:i0:i32 = load %l1:i32*
  %b5:i1:i32 = add %b5:i0:i32 2:i32
  %b5:i2:unit = store %b5:i1:i32 %l1:i32*
  %b5:i3:unit = store %b5:i1:i32 %l4:i32*
  j b6()

block b6:
  %b6:i0:i32 = load %l4:i32*
  %b6:i1:unit = store %b6:i0:i32 %l5:i32*
  j b3()

block b7:
  %b7:i0:unit = store 0:i32 %l0:i32*
  j b8()

block b8:
  %b8:i0:i32 = load %l0:i32*
  %b8:i1:i32 = load %l6:i32*
  %b8:i2:u1 = cmp lt %b8:i0:i32 %b8:i1:i32
  br %b8:i2:u1, b9(), b11()

block b9:
  %b9:i0:i32 = load %l0:i32*
  %b9:i1:i32 = mod %b9:i0:i32 2:i32
  %b9:i2:u1 = cmp ne %b9:i1:i32 0:i32
  br %b9:i2:u1, b12(), b13()

block b10:
  %b10:i0:i32 = load %l0:i32*
  %b10:i1:i32 = mod %b10:i0:i32 2:i32
  %b10:i2:u1 = cmp ne %b10:i1:i32 0:i32
  br %b10:i2:u1, b15(), b16()

block b11:
  %b11:i0:i32 = load %l1:i32*
  ret %b11:i0:i32

block b12:
  %b12:i0:i32 = load %l1:i32*
  %b12:i1:i32 = load %l2:i32*
  %b12:i2:i32 = add %b12:i0:i32 %b12:i1:i32
  %b12:i3:unit = store %b12:i2:i32 %l1:i32*
  j b14()

block b13:
  %b13:i0:i32 = load %l1:i32*
  %b13:i1:i32 = load %l3:i32*
  %b13:i2:i32 = add %b13:i0:i32 %b13:i1:i32
  %b13:i3:unit = store %b13:i2:i32 %l1:i32*
  j b14()

block b14:
  j b10()

block b15:
  %b15:i0:i32 = load %l0:i32*
  %b15:i1:i32 = add %b15:i0:i32 2:i32
  %b15:i2:unit = store %b15:i1:i32 %l0:i32*
  %b15:i3:unit = store %b15:i1:i32 %l7:i32*
  j b17()

block b16:
  %b16:i0:i32 = load %l0:i32*
  %b16:i1:i32 = add %b16:i0:i32 1:i32
  %b16:i2:unit = store %b16:i1:i32 %l0:i32*
  %b16:i3:unit = store %b16:i1:i32 %l7:i32*
  j b17()

block b17:
  %b17:i0:i32 = load %l7:i32*
  j b8()

block b18:
  ret 0:i32
}
// CHECK-LABEL: fun i32 @main () {
// CHECK-NEXT: init:
// CHECK-NEXT:   bid: b0
// CHECK-NEXT:   allocations:
// CHECK-NEXT:     %l0:i32:i
// CHECK-NEXT:     %l1:i32:p
// CHECK-NEXT:     %l2:i32:q
// CHECK-NEXT:     %l3:i32:r
// CHECK-NEXT:     %l4:i32:t0
// CHECK-NEXT:     %l5:i32:t1
// CHECK-NEXT:     %l6:i32:loop_num
// CHECK-NEXT:     %l7:i32:t2

// CHECK-LABEL: block b0:
// CHECK-NEXT:   %b0:i0:unit = store 2:i32 %l1:i32*
// CHECK-NEXT:   %b0:i1:unit = store 5:i32 %l2:i32*
// CHECK-NEXT:   %b0:i2:u1 = cmp ne 0:i32 0:i32
// CHECK-NEXT:   br %b0:i2:u1, b1(), b2()

// CHECK-LABEL: block b1:
// CHECK-NEXT:   %b1:i0:i32 = load %l1:i32*
// CHECK-NEXT:   %b1:i1:i32 = load %l2:i32*
// CHECK-NEXT:   %b1:i2:u1 = cmp gt %b1:i0:i32 %b1:i1:i32
// CHECK-NEXT:   br %b1:i2:u1, b4(), b5()

// CHECK-LABEL: block b2:
// CHECK-NEXT:   %b2:i0:i32 = load %l1:i32*
// CHECK-NEXT:   %b2:i1:i32 = load %l2:i32*
// CHECK-NEXT:   %b2:i2:i32 = add %b2:i0:i32 %b2:i1:i32
// CHECK-NEXT:   %b2:i3:unit = store %b2:i2:i32 %l5:i32*
// CHECK-NEXT:   j b3()

// CHECK-LABEL: block b3:
// CHECK-NEXT:   %b3:i0:i32 = load %l5:i32*
// CHECK-NEXT:   %b3:i1:unit = store %b3:i0:i32 %l3:i32*
// CHECK-NEXT:   %b3:i2:i32 = load @nonce:i32*
// CHECK-NEXT:   %b3:i3:i32 = mod %b3:i2:i32 100:i32
// CHECK-NEXT:   %b3:i4:unit = store %b3:i3:i32 %l6:i32*
// CHECK-NEXT:   %b3:i5:unit = store 0:i32 %l0:i32*
// CHECK-NEXT:   j b8()

// CHECK-LABEL: block b4:
// CHECK-NEXT:   %b4:i0:i32 = load %l1:i32*
// CHECK-NEXT:   %b4:i1:i32 = sub %b4:i0:i32 2:i32
// CHECK-NEXT:   %b4:i2:unit = store %b4:i1:i32 %l1:i32*
// CHECK-NEXT:   %b4:i3:unit = store %b4:i1:i32 %l4:i32*
// CHECK-NEXT:   j b6()

// CHECK-LABEL: block b5:
// CHECK-NEXT:   %b5:i0:i32 = load %l1:i32*
// CHECK-NEXT:   %b5:i1:i32 = add %b5:i0:i32 2:i32
// CHECK-NEXT:   %b5:i2:unit = store %b5:i1:i32 %l1:i32*
// CHECK-NEXT:   %b5:i3:unit = store %b5:i1:i32 %l4:i32*
// CHECK-NEXT:   j b6()

// CHECK-LABEL: block b6:
// CHECK-NEXT:   %b6:i0:i32 = load %l4:i32*
// CHECK-NEXT:   %b6:i1:unit = store %b6:i0:i32 %l5:i32*
// CHECK-NEXT:   j b3()

// CHECK-LABEL: block b8:
// CHECK-NEXT:   %b8:i0:i32 = load %l0:i32*
// CHECK-NEXT:   %b8:i1:i32 = load %l6:i32*
// CHECK-NEXT:   %b8:i2:u1 = cmp lt %b8:i0:i32 %b8:i1:i32
// CHECK-NEXT:   br %b8:i2:u1, b9(), b11()

// CHECK-LABEL: block b9:
// CHECK-NEXT:   %b9:i0:i32 = load %l0:i32*
// CHECK-NEXT:   %b9:i1:i32 = mod %b9:i0:i32 2:i32
// CHECK-NEXT:   %b9:i2:u1 = cmp ne %b9:i1:i32 0:i32
// CHECK-NEXT:   br %b9:i2:u1, b12(), b13()

// CHECK-LABEL: block b11:
// CHECK-NEXT:   %b11:i0:i32 = load %l1:i32*
// CHECK-NEXT:   ret %b11:i0:i32

// CHECK-LABEL: block b12:
// CHECK-NEXT:   %b12:i0:i32 = load %l1:i32*
// CHECK-NEXT:   %b12:i1:i32 = load %l2:i32*
// CHECK-NEXT:   %b12:i2:i32 = add %b12:i0:i32 %b12:i1:i32
// CHECK-NEXT:   %b12:i3:unit = store %b12:i2:i32 %l1:i32*
// CHECK-NEXT:   j b14()

// CHECK-LABEL: block b13:
// CHECK-NEXT:   %b13:i0:i32 = load %l1:i32*
// CHECK-NEXT:   %b13:i1:i32 = load %l3:i32*
// CHECK-NEXT:   %b13:i2:i32 = add %b13:i0:i32 %b13:i1:i32
// CHECK-NEXT:   %b13:i3:unit = store %b13:i2:i32 %l1:i32*
// CHECK-NEXT:   j b14()

// CHECK-LABEL: block b14:
// CHECK-NEXT:   %b14:i0:i32 = load %l0:i32*
// CHECK-NEXT:   %b14:i1:i32 = mod %b14:i0:i32 2:i32
// CHECK-NEXT:   %b14:i2:u1 = cmp ne %b14:i1:i32 0:i32
// CHECK-NEXT:   br %b14:i2:u1, b15(), b16()

// CHECK-LABEL: block b15:
// CHECK-NEXT:   %b15:i0:i32 = load %l0:i32*
// CHECK-NEXT:   %b15:i1:i32 = add %b15:i0:i32 2:i32
// CHECK-NEXT:   %b15:i2:unit = store %b15:i1:i32 %l0:i32*
// CHECK-NEXT:   %b15:i3:unit = store %b15:i1:i32 %l7:i32*
// CHECK-NEXT:   j b17()

// CHECK-LABEL: block b16:
// CHECK-NEXT:   %b16:i0:i32 = load %l0:i32*
// CHECK-NEXT:   %b16:i1:i32 = add %b16:i0:i32 1:i32
// CHECK-NEXT:   %b16:i2:unit = store %b16:i1:i32 %l0:i32*
// CHECK-NEXT:   %b16:i3:unit = store %b16:i1:i32 %l7:i32*
// CHECK-NEXT:   j b17()

// CHECK-LABEL: block b17:
// CHECK-NEXT:   %b17:i0:i32 = load %l7:i32*
// CHECK-NEXT:   j b8()
// CHECK-NEXT: }
