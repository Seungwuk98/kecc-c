// RUN: kecc-opt %s --canonicalize-constant --gvn | FileCheck %s

fun unit @sink (i32) {
// CHECK: fun unit @sink (i32)
init:
  bid: b0
  allocations:
// CHECK-NEXT: init:
// CHECK-NEXT:   bid: b0
// CHECK-NEXT:   allocations:

block b0:
// CHECK-LABEL: block b0:
  %b0:p0:i32
  // CHECK-NEXT: %b0:p0:i32
  ret unit:unit
  // CHECK-NEXT: ret unit:unit
}

fun i32 @gvn (i32) {
// CHECK-LABEL: fun i32 @gvn (i32)
init:
  bid: b0
  allocations:
// CHECK-NEXT: init:
// CHECK-NEXT:   bid: b0
// CHECK-NEXT:   allocations:

block b0:
// CHECK-LABEL: block b0:
  %b0:p0:i32:a
  %b0:i0:i32:b = add %b0:p0:i32 1:i32
  br undef:i1, b1(), b2()
  // CHECK-NEXT: %b0:p0:i32:a
  // CHECK-NEXT: %b0:i0:i32:b = add %b0:p0:i32 1:i32
  // CHECK-NEXT: br undef:i1, b1(), b2()

block b1:
// CHECK-LABEL: block b1:
  %b1:i0:i32:b = add %b0:p0:i32 1:i32
  %b1:i1:i32:c = add %b1:i0:i32 2:i32
  // CHECK-NEXT: %b1:i0:i32:b = add %b0:p0:i32 1:i32
  // CHECK-NEXT: %b1:i1:i32:c = add %b0:i0:i32 2:i32

  %b1:i2:unit = call @sink:[ret:unit params:(i32)]*(%b1:i0:i32)
  // CHECK-NEXT: %b1:i2:unit = call @sink:[ret:unit params:(i32)]*(%b0:i0:i32)

  j b3()
  // CHECK-NEXT: j b3(%b1:i1:i32)

block b2:
// CHECK-LABEL: block b2:
  %b2:i0:i32:c = add %b0:i0:i32 2:i32
  // CHECK-NEXT: %b2:i0:i32:c = add %b0:i0:i32 2:i32
  %b2:i1:unit = call @sink:[ret:unit params:(i32)]*(%b2:i0:i32)
  // CHECK-NEXT: %b2:i1:unit = call @sink:[ret:unit params:(i32)]*(%b2:i0:i32)

  j b3()
  // CHECK-NEXT: j b3(%b2:i0:i32)

block b3:
// CHECK-LABEL: block b3:
  // CHECK-NEXT: %b3:p0:i32
  %b3:i0:i32:b = add %b0:p0:i32 1:i32
  // CHECK-NEXT: %b3:i0:i32:b = add %b0:p0:i32 1:i32
  %b3:i1:i32:c = add %b3:i0:i32 2:i32
  // CHECK-NEXT: %b3:i1:i32:c = add %b0:i0:i32 2:i32

  %b3:i2:unit = call @sink:[ret:unit params:(i32)]*(%b3:i0:i32)
  // CHECK-NEXT: %b3:i2:unit = call @sink:[ret:unit params:(i32)]*(%b0:i0:i32)
  %b3:i3:unit = call @sink:[ret:unit params:(i32)]*(%b3:i1:i32)
  // CHECK-NEXT: %b3:i3:unit = call @sink:[ret:unit params:(i32)]*(%b3:p0:i32)

  ret 0:i32
  // CHECK-NEXT: ret 0:i32
}
// CHECK-NEXT: }
