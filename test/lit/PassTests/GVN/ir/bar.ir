// RUN: kecc-opt %s --canonicalize-constant --gvn | FileCheck %s


fun i32 @bar (i32, i32, i32) {
init:
  bid: b0
  allocations: 


block b0:
  %b0:p0:i32:x
  %b0:p1:i32:y
  %b0:p2:i32:z
  %b0:i0:u1 = cmp eq %b0:p0:i32 %b0:p1:i32
  br %b0:i0:u1, b1(), b2()

block b1:
  ret %b0:p1:i32

block b2:
  ret %b0:p2:i32
}
// CHECK: fun i32 @bar (i32, i32, i32) {
// CHECK-NEXT: init:
// CHECK-NEXT:   bid: b0
// CHECK-NEXT:   allocations:


// CHECK-LABEL: block b0:
// CHECK-NEXT:   %b0:p0:i32:x
// CHECK-NEXT:   %b0:p1:i32:y
// CHECK-NEXT:   %b0:p2:i32:z
// CHECK-NEXT:   %b0:i0:u1 = cmp eq %b0:p0:i32 %b0:p1:i32
// CHECK-NEXT:   br %b0:i0:u1, b1(), b2()

// CHECK-LABEL: block b1:
// CHECK-NEXT:   ret %b0:p1:i32

// CHECK-LABEL: block b2:
// CHECK-NEXT:   ret %b0:p2:i32
// CHECK-NEXT: }

fun i32 @main () {
init:
  bid: b0
  allocations: 


block b0:
  ret 1:i32
}
// CHECK-LABEL: fun i32 @main () {
// CHECK-NEXT: init:
// CHECK-NEXT:   bid: b0
// CHECK-NEXT:   allocations:


// CHECK-LABEL: block b0:
// CHECK-NEXT:   ret 1:i32
// CHECK-NEXT: }
