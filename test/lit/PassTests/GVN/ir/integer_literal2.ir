// RUN: kecc-test-executor %s --test-return-value=1
// RUN: kecc-opt %s --canonicalize-constant --gvn | FileCheck %s


fun i32 @main () {
init:
  bid: b0
  allocations: 


block b0:
  %b0:i0:u32 = typecast 0:i32 to u32
  %b0:i1:u1 = cmp lt %b0:i0:u32 4294967295:u32
  %b0:i2:i32 = typecast %b0:i1:u1 to i32
  ret %b0:i2:i32
}
// CHECK: fun i32 @main () {
// CHECK-NEXT: init:
// CHECK-NEXT:   bid: b0
// CHECK-NEXT:   allocations:


// CHECK-LABEL: block b0:
// CHECK-NEXT:   %b0:i0:u32 = typecast 0:i32 to u32
// CHECK-NEXT:   %b0:i1:u1 = cmp lt %b0:i0:u32 4294967295:u32
// CHECK-NEXT:   %b0:i2:i32 = typecast %b0:i1:u1 to i32
// CHECK-NEXT:   ret %b0:i2:i32
// CHECK-NEXT: }
